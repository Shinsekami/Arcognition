{
  "name": "google-reverse-image-api",
  "version": "0.2.3",
  "description": "A simple Express.js and Puppeteer app that includes a reverse API to search for images using Google Reverse Image Search.",
  "keywords": [
    "api",
    "google",
    "reverse",
    "google reverse image",
    "vercel"
  ],
  "main": "src/app.js",
  "scripts": {
    "start": "node build/app.js",
    "dev": "nodemon --exec \"node --trace-warnings start.js\"",
    "build": "npm run clean && tsc && npm run copy-assets",
    "clean": "rimraf build"
  },
  "author": "SOME-1HING",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/vision": "^5.2.0",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsdom": "^26.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7"
  }
}
